set-name example.com/node-hello
dep add quay.io/coreos/alpine-sh
run -- apk add --update nodejs
copy install /src
set-working-directory /src
run -- source /src/install.sh
set-exec -- source run.sh
port add ssh tcp 22
port add www tcp 8080
port add smtp tcp 25
port add smtps tcp 587
port add imap tcp 993
port add pop tcp 995


